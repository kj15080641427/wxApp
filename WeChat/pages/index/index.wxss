.index-layout{
  display: flex;
  justify-content: space-between;
  align-content: center;
}
.index-layout-center{
  display: flex;
  justify-content: center;
  align-items: center;
}
.index-tips{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 25rpx;
  width: 25rpx;
  border-radius: 50%;
  background-color: red;
  font-size: 18rpx;
  color: white;
  position: absolute;
  margin-left: 710rpx;
  margin-top: 20rpx;
}
.index-input-height{
  height: 100rpx;
  background-color: #1ec88c;
}
.index-search{
  display: flex;
  align-items: center;
  height: 70rpx;
  width: 600rpx;
  margin-left: 30rpx;
  background-color: #32b987;
  margin-top: 15rpx;
  padding-left: 20rpx;
  font-size: 28rpx;
  border-radius: 10rpx;
}
.index-news{
  width: 50rpx;
  height:40rpx;
  background: url('../../image/homeicon_3x/home page_02_3x.png') no-repeat;
  background-size: 100%;
  margin-top: 30rpx;
  margin-right: 30rpx;
  position:relative;
}
.index-ml{
  margin-left: 40rpx;
}
.index-mr{
  margin-right: 40rpx;
}
.index-green-line{
  height: 35rpx;
  width: 8rpx;
  background-color: #1ec88c;
  border-radius: 5rpx;
  margin-top: 5rpx;
  margin-right: 10rpx;
}
.index-text-style{
  font-size: 36rpx;
  color: #323232;
  font-weight: bold;
  display: flex;
  justify-content: center;
}
.index-text-style-mini{
  font-size: 24rpx;
  margin-top: 30rpx;
}
.index-text-style-green{
  color: #1ec88c;
}
.index-text-style-gray{
  color: #b5b5b6;
}
.index-image{
  width: 650rpx;
  height: 200rpx;
  margin-top: 65rpx;
  /* margin-left: 50rpx; */
  /* display: flex;
  justify-content: center;  */
}
.index-service{
  display: flex;
  justify-content: space-between;
  margin-left: 35rpx;
  margin-right: 35rpx;
}
.index-service-left{
  width:90rpx;
  height:90rpx;
  background-color:#1ec88c;
  margin-top:30rpx
}
.index-service-right{
  font-size:24rpx;
  color:#b5b5b6;
  margin-top:30rpx
}
.index-tab{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #b5b5b6;
}
.index-tab-margin{
  margin-top: 40rpx;
  margin-left: 35rpx;
  margin-right: 35rpx;
}
.index-more{
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #b5b5b6;
}
@font-face {
    font-family: 'PingFang SC';
    src: url('PingFangSCRegular.eot');
    font-weight: normal;
    font-style: normal;
}
.index-lawyer-icon{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: red;
  margin-top: 6rpx;
  /* display: flex;
  justify-content: space-around; */
}
.index-lawyer-remark{
  display: flex;
  /* justify-content: space-around; */
  font-size: 24rpx;
  color: #b5b5b6;
}
.index-lawyer-name{
  /* display: flex;
  justify-content: space-between; */
  font-size: 28rpx;
  color: #323232
}
.scroll-x{
  height:80rpx;
  white-space:nowrap;
  display:flex;
  /* margin-left: 20rpx; */
  margin-right:20rpx;
}
/* 隐藏scrollbar */
/* ::-webkit-scrollbar{
  width: 0;
  height: 0;
} */
.view-parent{
  display:inline-block;
}
.view-item{
  width:50px;
  /* height:50px; */
  display:flex;
  align-items:center;
  justify-content:center;
  font-size:28rpx;
  /* font-color:#222; */
  /* background:#80CBC4; */
  margin-left:20rpx;
  margin-right: 20rpx;
}
.index-article{
  display: flex;
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
}
.index-article-text{
  font-size: 32rpx;
  width: 457rpx;
}
.index-article-icon{
  width: 223rpx;
  height: 145rpx;
  background-color: red;
}
.index-article-text-lower{
  font-size: 24rpx;
  color: #b5b5b6;
  margin-top: 40rpx;
}
.modal-dialog{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 550rpx;
  height: 150rpx;
  margin-left: 100rpx;
  border: outset;
  border-width: 2rpx;
  border-radius: 10rpx;
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
/*对话框 */
.commodity_attr_box {
  height: 300rpx;
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding-top: 20rpx;
}





/* 字体代码 */
@font-face {
    font-family: 'PingFang SC';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('PingFangSCRegular.ttf') format('truetype'),
        url('PingFangSCRegular.svg#PingFangSCRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}

