      <image class='my-page-avatar' src='{{userInfo.iconImage}}' bindtap='login'></image>
      <view class='my-page-editinfo-flex' bindtap='gotoEditInfo'>
        <view class='my-page-editinfo'>编辑信息</view>
        <image src='../../image/homeicon_3x/homepage_09_3x.png' style='width:25rpx;height:25rpx;padding-top:5rpx' />
      </view>
    </view>
    <view class='my-page-textgenter'>
      <view class='my-page-text' style='font-weight:bold'>{{userInfo.memberName}}</view>
      <view class='my-page-gender'>
        <image src='../../image/my_icon@3x/mine_icon_04_3x.png' style='width:20rpx;height:20rpx'></image>
        <text>18岁</text>
      </view>
    </view>
    <view class='my-page-text' style='font-size:30rpx'>湖南常德</view>
    <view class='my-page-unions'>
      <view class='my-page-unions-detail'>中国浙商会湖南分会</view>
      <view class='my-page-unions-detail'>中国浙商会湖南分会</view>
    </view>
  </view>
  </view>


