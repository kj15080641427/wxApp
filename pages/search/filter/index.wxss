/* pages/search/filter/index.wxss */
.filter-background{
  background-color: white;
  padding-left: 35rpx;
}
.filter-page-line{
  margin-left: 35rpx;
  margin-right: 35rpx;
  height: 1rpx;
  background-color: #f4f4f4;
}
.filter-page-top{
  height: 100rpx;
  display: flex;
  align-items: center;
  color: #b5b5b5;
  font-size: 28rpx;
}
.filter-page-frame-view{
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-wrap: wrap ;
  padding-top:30rpx;
}
.filter-page-frame-color{
  width: 140rpx;
  height: 50rpx;
  border-radius: 10rpx;
  margin-right: 35rpx;
  margin-top: 20rpx;
  color: white;
  background-color: #1ec88c
}
.filter-page-frame{
  width: 140rpx;
  height: 50rpx;
  border: 1rpx solid #b5b5b5;
  border-radius: 10rpx;
  color: #b5b5b5;
  margin-right: 35rpx;
  margin-top: 20rpx;
}
.filter-page-frame-text{
  display: flex;
  justify-content: center;
  line-height: 50rpx;
  font-size: 28rpx;
}
.filter-page-gender-top{
  height: 100rpx;
  width: 750rpx;
  margin-top: 35rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #b5b5b5
}
.filter-page-gender-view{
  height: 100rpx;
  display: flex;
  align-items: center;
  margin-bottom: 35rpx;
}
.filter-page-gender{
  border: 1rpx solid #b5b5b5;
  border-radius: 10rpx;
  height: 50rpx;
  width: 140rpx;
  color: #b5b5b5;
  margin-right: 35rpx;
}
.filter-page-gender-color{
  border-radius: 10rpx;
  height: 50rpx;
  width: 140rpx;
  color: white;
  background-color: #1ec88c;
  margin-right: 35rpx;
}
.filter-page-cneter{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  /* margin-top: 35rpx; */
  /* line-height: 100rpx; */
  color: #b5b5b5;
  font-size: 28rpx;
}
.filter-page-icon{
  width: 30rpx;
  height: 30rpx;
  margin-right: 30rpx;
}
.filter-page-button-view{
  position:fixed;
  bottom: 0;
  background-color: white;
  margin-top: 35rpx;
  height: 140rpx;
  width: 750rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
button{
  width: 220rpx;
  height: 94rpx;
  border-radius: 47rpx;
  background-color: #1ec88c;
  font-size: 36rpx;
  color: white;
  line-height: 94rpx;
}
.filter-page-picker{
  width:660rpx;
  height:60rpx;
  line-height:60rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.filter-page-picker-text{
  margin-left:35rpx;
  color:black;
}
